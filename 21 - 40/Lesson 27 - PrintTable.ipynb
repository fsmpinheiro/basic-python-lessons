   "source": [
    "\n",
    "def number_table(number, limit=10):\n",
    "    print(f\"Multiplication table of {number}: \")\n",
    "    for i in range(1, limit + 1):\n",
    "        print(f\"{number} x {i} = {number * i}\")"
   ]
  },
