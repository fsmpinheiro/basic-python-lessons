   "source": [
    "\n",
    "def perfect_number(number):\n",
    "    return number > 0 and sum(i for i in range(1, number) if number % i == 0) == number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "f4034642-70e8-475d-8aab-cdcf60b56814",
   "metadata": {},
